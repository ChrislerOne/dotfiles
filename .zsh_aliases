# Load shared aliases
local alias_file="/Users/christopherlewerenz/dotfiles/.aliases"
if [ -f "$alias_file" ]; then
    while IFS='=' read -r key value; do
        # Skip comments and empty lines
        [[ "$key" =~ ^[[:space:]]*# || -z "$key" ]] && continue
        alias "$key"="$value"
    done < "$alias_file"
fi
